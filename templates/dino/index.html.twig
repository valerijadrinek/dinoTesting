 {% extends 'base.html.twig' %} 

 {% block body %}
 <div class="jumbotron jumbotron-fluid rounded bg-info text-white my-2 my-lg-5 py-2 py-lg-4">
     <div class="container text-center ">
        <h1 class="display-4">Welcome to the Dinoland</h1>
        <p class="lead">This page shows schedule of dinosours at their habitat...</p>
     </div>
</div>
 <header></header>
     <div class="table-responsive-md">
        <table class="table table-hover">
            <thead>
                <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">Genus</th>
                <th scope="col">Length</th>
                <th scope="col">Enclosure</th>
                </tr>
            </thead>
            <tbody>
                
                {% for dino in dinos %}
                <tr>
                    <th scope="row"></th>
                    <td>{{ dino.name }}</td>
                    <td>{{ dino.genus }}</td>
                    <td>{{ dino.length }}</td>
                    <td>{{ dino.enclosure }}</td>
                </tr>
                {% endfor %}
               
            </tbody>
            </table>
     </div>

     <div class="jumbotron jumbotron-fluid rounded bg-info text-white my-2 my-lg-5 py-2 py-lg-4">
     <div class="container text-center ">
        <h1 class="display-4">Goodbye from the Dinoland</h1>
        <p class="lead">I hope we will see You soon ...</p>
     </div>
</div>
 {% endblock %}